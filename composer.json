{
    "name": "martyb/laravel-html",
    "description": "A fluent html builder based on the laravel-html package by Spatie bvba",
    "keywords": [
        "spatie",
        "martyb",
        "laravel",
        "html"
    ],
    "homepage": "https://github.com/martyvdb/laravel-html",
    "license": "MIT",
    "authors": [
        {
            "name": "Martijn van den Berg",
            "email": "info@codesmashers.com",
            "homepage": "https://codesmashers.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/http": "~5.4.0|~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.4.0|~5.5.0|~5.6.0|~5.7.0"
    },
    "autoload": {
        "psr-4": {
            "Martyb\\Html\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Martyb\\Html\\HtmlServiceProvider"
            ],
            "aliases": {
                "Html": "Martyb\\Html\\Facades\\Html"
            }
        }
    }
}
